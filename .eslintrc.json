{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2019
    },
    "rules": {
        "no-console": [0],
        "no-use-before-define": [0],
        "no-param-reassign": [0]
    }
}
